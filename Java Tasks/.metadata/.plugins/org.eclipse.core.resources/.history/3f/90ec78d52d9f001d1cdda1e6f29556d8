package Collections1;
import java.util.TreeSet;

public class Main {
    public static void main(String[] args) {
        TreeSet<Employee> employees = new TreeSet<Employee>();

        employees.add(new Employee("John", 30, 5000));
        employees.add(new Employee("Mike", 35, 6000));
        employees.add(new Employee("Jessica", 25, 4000));
        employees.add(new Employee("Adam", 40, 7000));
        employees.add(new Employee("Amy", 27, 4500));

        for (Employee e : employees) {
            System.out.println(e.getName() + " - " + e.getAge() + " - " + e.getSalary());
        }
    }
}