package Collections1;

public class EmployeeData {
int id;
String name; 
int age;
String gender;
String department;
int yearOfJoining;
double salary;
public EmployeeData(String name, int age, double salary) 
{
    this.name = name;
    this.age = age;
    this.salary = salary;
}
public String getName() 
{
    return name;
}
public double getSalary() 
{
    return salary;
}
@Override
public String toString() 
{
    return  ", Name : "+name
            +", age : "+age
            +", Salary : "+salary;
}
}