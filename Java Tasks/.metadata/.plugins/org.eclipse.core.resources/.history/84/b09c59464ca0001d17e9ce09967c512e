package Collections1;

import java.util.Collections;
import java.util.Comparator;

public class EmployeeStructure implements Comparable<EmployeeStructure> {
    private String name;
    private int age;
    private double salary;

    public EmployeeStructure(String name, int age, double salary) {
        this.name = name;
        this.age = age;
        this.salary = salary;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public double getSalary() {
        return salary;
    }
   // public String toString() {
     //   return EmployeeStructure.this.getName();
    //}
    
    @Override
    public int compareTo(EmployeeStructure employee) {
        return this.name.compareTo(employee.name);
    }
    
}